{
  "version": "0.1.0",
  "tasks": [{
    "command": "gulp",
    "isShellCommand": true,
    "taskName": "tslint",
    "args": ["tslint"],
    "problemMatcher": "$tslint5"
  }, {
    "command": "dotnet",
    "isShellCommand": true,
    "taskName": "build",
    "args": [
      "build", "${workspaceRoot}/Vega.csproj"
    ],
    "isBuildCommand": true,
    "problemMatcher": "$msCompile"
  }]
}
